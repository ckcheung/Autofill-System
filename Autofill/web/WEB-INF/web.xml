<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Controller</servlet-name>
        <servlet-class>Autofill.Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataSaver</servlet-name>
        <servlet-class>Autofill.DataSaver</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GroupSaver</servlet-name>
        <servlet-class>Autofill.GroupSaver</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controller</servlet-name>
        <url-pattern>/Controller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataSaver</servlet-name>
        <url-pattern>/DataSaver</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GroupSaver</servlet-name>
        <url-pattern>/GroupSaver</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Controller</welcome-file>
    </welcome-file-list>
</web-app>
